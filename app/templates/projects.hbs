Projects:
<table class="table table-striped">
  <thead>
    <tr>
      <th>Name</th>
      <th>Start</th>
      <th>End</th>
    </tr>
  </thead>
  <tbody>
    {{#each model as |project|}}
    <tr>
      <td>{{project.name}}</td>
      <td>{{project.start}}</td>
      <td>{{project.finish}}</td>
    </tr>
    {{/each}}
  </tbody>
</table>
{{outlet}}
